-card-title>\n        <v-divider></v-divider>\n        <v-text-field class=\"input\" style=\"margin-left:25px; margin-right: 30px\" v-model=\"nom\" color=\"orange\" label=\"Entrez film\" required></v-text-field>\n        <v-btn color=\"orange\" class=\"mr-4\" @click=\"searchFilm\">Validate</v-btn>\n        <br><br>\n        <v-row justify=\"center\">\n          <v-col cols=\"3\" style=\"width:80%;margin-left: 10px; margin-right: 10px\" v-for=\"(info,index) in Infos\" v-bind:key=\"info.id\" v-bind:item=\"info\" v-bind:index=\"index\">\n            <v-card class=\"white--text align-end\" max-width=\"500\" max-height=\"500\">\n              <v-img v-bind:src=\"'https://image.tmdb.org/t/p/original' + info.poster_path\" style=\"cursor:pointer\" height=\"300\" width=\"350\" @click =\"ajoutFilm(index)\"></v-img>\n                <v-card-text class=\"text--primary\">                              \n                  <h6>{{info.original_title}}</h6>\n                </v-card-text>\n            </v-card>\n          </v-col>\n        </v-row>          \n      </v-card>\n    </v-dialog>\n  </v-container>\n</template>\n\n<script>\n  import movieApi from '@/services/filmApi'\n  \n  export default {\n    data () {\n      return {\n        search: false,\n        nom: '',\n        Infos: []\n      }\n    },\n\n    mounted() {\n      movieAPI.searchFilm(this.nom) \n        .then(response => {\n          this.Infos = response\n        })\n        .catch(error => {\n          console.log(error)\n        })\n      \n    }\n  }\n  \n</script>\n\n<style scoped>\n  \n</style>"]}]}